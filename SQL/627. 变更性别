//https://leetcode.cn/problems/swap-salary/

# Write your MySQL query statement below
UPDATE Salary
SET
    sex = CASE sex
        WHEN 'm' THEN 'f'
        ELSE 'm'
    END;


# Write your MySQL query statement below
update salary
set sex = (
    case sex 
    when 'm' then 'f'
    else 'm'
    end
);
